generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("NEON_DATABASE_URI")
}

model User {
  id      Int    @id @default(autoincrement())
  name    String
  surname String
}

model Media {
  name      String    @id
  mime_type String
  data      Bytes
}